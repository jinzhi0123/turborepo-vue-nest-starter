{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": ["./base.json"],
  "compilerOptions": {
    "jsx": "preserve",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  }
}
